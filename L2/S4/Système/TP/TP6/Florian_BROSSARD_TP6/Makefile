# Generated by generateMakefile.sh

CC=gcc
CFLAGS=-Wall -std=c99 -o2
EXEC_LIST=kill launch myself system 

all: kill launch myself system 

kill: kill.c
	${CC} ${CFLAGS} -o $@ kill.c

launch: launch.c
	${CC} ${CFLAGS} -o $@ launch.c

myself: myself.c
	${CC} ${CFLAGS} -o $@ myself.c

system: system.c
	${CC} ${CFLAGS} -D_POSIX_C_SOURCE=200809L -o $@ system.c

clean:
	 rm -f ${EXEC_LIST}

