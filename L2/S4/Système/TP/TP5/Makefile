# Generated by generateMakefile.sh

CC=gcc
CFLAGS=-Wall -std=c99 -o2
EXEC_LIST=cut_perso head_perso ls_perso 

all: cut_perso head_perso ls_perso 

cut_perso: cut_perso.c
	${CC} ${CFLAGS} -o $@ cut_perso.c

head_perso: head_perso.c
	${CC} ${CFLAGS} -o $@ head_perso.c

ls_perso: ls_perso.c
	${CC} ${CFLAGS} -lbsd -o $@ ls_perso.c

clean:
	 rm -f ${EXEC_LIST}

