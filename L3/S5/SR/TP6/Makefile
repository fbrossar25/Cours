all: shared_memory_clean shared_memory_read shared_memory_thread shared_memory_write 

shared_memory_clean: shared_memory_clean.c
	${CC} ${CFLAGS} -o $@ shared_memory_clean.c ${LDFLAGS}

shared_memory_read: shared_memory_read.c
	${CC} ${CFLAGS} -o $@ shared_memory_read.c ${LDFLAGS}

shared_memory_thread: shared_memory_thread.c
	${CC} ${CFLAGS} -o $@ shared_memory_thread.c ${LDFLAGS} -lpthread

shared_memory_write: shared_memory_write.c
	${CC} ${CFLAGS} -o $@ shared_memory_write.c ${LDFLAGS}

clean:
